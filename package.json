{
  "name": "ros3d-evolve",
  "description": "The standard ROS Javascript Visualization Library",
  "version": "1.2.3",
  "main": "./dist/ros3d.cjs.js",
  "module": "./dist/ros3d.es.js",
  "browser": "./dist/ros3d.iife.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "docs": "jsdoc -c jsdoc_conf.json",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:verify": "npm run build && vitest run",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "loglevel": "^1.9.2",
    "loglevel-plugin-prefix": "^0.8.4"
  },
  "peerDependencies": {
    "eventemitter3": "^5.0.1",
    "roslib": "^2.0.1",
    "three": "^0.118.3"
  },
  "devDependencies": {
    "@vitest/ui": "^4.0.5",
    "jsdom": "^26.1.0",
    "terser": "^5.14.2",
    "vite": "npm:rolldown-vite@7.1.14",
    "vitest": "^4.0.5"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/ros3d.es.js",
      "require": "./dist/ros3d.cjs.js"
    }
  },
  "keywords": [
    "ROS",
    "ros",
    "roslib",
    "roslibjs",
    "ros3d",
    "ros3djs",
    "robot"
  ],
  "directories": {
    "example": "examples"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.14"
    }
  }
}
